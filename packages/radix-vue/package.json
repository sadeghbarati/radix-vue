{
  "name": "radix-vue",
  "type": "module",
  "version": "0.4.1",
  "description": "Vue port for Radix UI Primitives.",
  "author": "Radix Vue Contributors (https://github.com/radix-vue)",
  "license": "MIT",
  "homepage": "https://github.com/radix-vue/radix-vue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/radix-vue/radix-vue.git"
  },
  "bugs": {
    "url": "https://github.com/radix-vue/radix-vue/issues"
  },
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./nuxt": {
      "default": "./dist/nuxt/index.cjs",
      "import": "./dist/nuxt/index.mjs"
    },
    "./resolver": {
      "import": "./dist/resolver/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist",
    "./LICENSE"
  ],
  "scripts": {
    "build": "pnpm type-check && pnpm build-only && pnpm type-gen",
    "build-only": "vite build",
    "type-check": "vue-tsc -p tsconfig.check.json --noEmit",
    "type-gen": "vue-tsc --declaration --emitDeclarationOnly -p ./tsconfig.build.json && api-extractor run && rimraf temp",
    "test": "vitest",
    "pub:release": "pnpm publish --access public"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.4.2",
    "@floating-ui/vue": "^1.0.1"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@microsoft/api-extractor": "^7.38.0",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^7.0.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.18",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitest/coverage-istanbul": "^0.33.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/components": "^10.4.1",
    "@vueuse/core": "^10.4.1",
    "@vueuse/shared": "^10.4.1",
    "aria-hidden": "^1.2.3",
    "defu": "^6.1.2",
    "jsdom": "^22.1.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vitest": "^0.34.3",
    "vitest-axe": "0.1.0",
    "vitest-canvas-mock": "^0.3.3",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.19"
  }
}
